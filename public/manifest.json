{
  "manifest_version": 2,
  "name": "Ideas for Environment",
  "version": "1.0.0",
  "description": "Practical advices for environment protection",
  "content_security_policy": "script-src 'self' 'sha256-...<read from console>'; object-src 'self'",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}